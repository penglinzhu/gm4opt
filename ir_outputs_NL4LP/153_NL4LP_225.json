{
  "meta": {
    "problem_id": "NL4LP_225",
    "source": null,
    "description": "Optimize the number of shifts on bikes and scooters to maximize tips.",
    "sense": "max",
    "version": 1
  },
  "sets": [
    {
      "name": "TRANSPORT",
      "elements": [
        "bike",
        "scooter"
      ],
      "description": "Types of transportation for shifts"
    }
  ],
  "params": [
    {
      "name": "orders_per_shift",
      "indices": [
        "TRANSPORT"
      ],
      "values": {
        "bike": 10,
        "scooter": 7
      },
      "description": "Number of orders delivered per shift"
    },
    {
      "name": "energy_per_shift",
      "indices": [
        "TRANSPORT"
      ],
      "values": {
        "bike": 5,
        "scooter": 6
      },
      "description": "Energy consumed per shift"
    },
    {
      "name": "tips_per_shift",
      "indices": [
        "TRANSPORT"
      ],
      "values": {
        "bike": 50,
        "scooter": 43
      },
      "description": "Tips received per shift"
    }
  ],
  "vars": [
    {
      "name": "shifts",
      "indices": [
        "TRANSPORT"
      ],
      "vartype": "integer",
      "lb": 0,
      "ub": null,
      "description": "Number of shifts scheduled for each type of transportation"
    }
  ],
  "objective": {
    "name": "max_total_tips",
    "sense": "max",
    "expr": "shifts['bike'] * tips_per_shift['bike'] + shifts['scooter'] * tips_per_shift['scooter']",
    "description": "Maximize total tips received from all shifts"
  },
  "constraints": [
    {
      "name": "total_shifts_limit",
      "expr_lhs": "shifts['bike'] + shifts['scooter']",
      "sense": "<=",
      "expr_rhs": "40",
      "description": "Total number of shifts cannot exceed 40"
    },
    {
      "name": "total_energy_limit",
      "expr_lhs": "shifts['bike'] * energy_per_shift['bike'] + shifts['scooter'] * energy_per_shift['scooter']",
      "sense": "<=",
      "expr_rhs": "230",
      "description": "Total energy consumption cannot exceed 230 units"
    },
    {
      "name": "minimum_orders_requirement",
      "expr_lhs": "shifts['bike'] * orders_per_shift['bike'] + shifts['scooter'] * orders_per_shift['scooter']",
      "sense": ">=",
      "expr_rhs": "320",
      "description": "Must deliver at least 320 orders"
    },
    {
      "name": "minimum_scooter_shifts",
      "expr_lhs": "shifts['scooter']",
      "sense": ">=",
      "expr_rhs": "5",
      "description": "Must have at least 5 shifts on scooters"
    }
  ],
  "graph": null
}