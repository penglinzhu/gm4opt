      "description": "Mint usage constraint"
    },
    {
      "name": "active_ingredient_constraint",
      "expr_lhs": "demos['1'] * active_ingredient_usage['1'] + demos['2'] * active_ingredient_usage['2']",
      "sense": "<=",
      "expr_rhs": "available_active_ingredient",
      "description": "Active ingredient usage constraint"
    },
    {
      "name": "black_tar_constraint",
