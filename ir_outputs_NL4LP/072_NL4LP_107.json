{
  "meta": {
    "problem_id": "NL4LP_107",
    "source": null,
    "description": "Optimize the number of batches of vitamin shots and pills to maximize the number of people supplied.",
    "sense": "max",
    "version": 1
  },
  "sets": [
    {
      "name": "PRODUCTS",
      "elements": [
        "shots",
        "pills"
      ],
      "description": "Types of vitamin products"
    }
  ],
  "params": [
    {
      "name": "vitamin_c_requirement",
      "indices": [
        "PRODUCTS"
      ],
      "values": {
        "shots": 30,
        "pills": 50
      },
      "description": "Vitamin C required per batch"
    },
    {
      "name": "vitamin_d_requirement",
      "indices": [
        "PRODUCTS"
      ],
      "values": {
        "shots": 40,
        "pills": 30
      },
      "description": "Vitamin D required per batch"
    },
    {
      "name": "people_supplied",
      "indices": [
        "PRODUCTS"
      ],
      "values": {
        "shots": 10,
        "pills": 7
      },
      "description": "Number of people supplied per batch"
    },
    {
      "name": "vitamin_c_available",
      "indices": [],
      "values": 1200,
      "description": "Total available units of vitamin C"
    },
    {
      "name": "vitamin_d_available",
      "indices": [],
      "values": 1500,
      "description": "Total available units of vitamin D"
    }
  ],
  "vars": [
    {
      "name": "batches",
      "indices": [
        "PRODUCTS"
      ],
      "vartype": "integer",
      "lb": 0.0,
      "ub": null,
      "description": "Number of batches to produce"
    }
  ],
  "objective": {
    "name": "max_people_supplied",
    "sense": "max",
    "expr": "batches['shots'] * people_supplied['shots'] + batches['pills'] * people_supplied['pills']",
    "description": "Maximize the number of people that can be supplied"
  },
  "constraints": [
    {
      "name": "vitamin_c_constraint",
      "expr_lhs": "batches['shots'] * vitamin_c_requirement['shots'] + batches['pills'] * vitamin_c_requirement['pills']",
      "sense": "<=",
      "expr_rhs": "vitamin_c_available",
      "description": "Vitamin C availability constraint"
    },
    {
      "name": "vitamin_d_constraint",
      "expr_lhs": "batches['shots'] * vitamin_d_requirement['shots'] + batches['pills'] * vitamin_d_requirement['pills']",
      "sense": "<=",
      "expr_rhs": "vitamin_d_available",
      "description": "Vitamin D availability constraint"
    },
    {
      "name": "pills_greater_than_shots",
      "expr_lhs": "batches['pills']",
      "sense": ">=",
      "expr_rhs": "batches['shots'] + 1",
      "description": "Number of batches of pills must be greater than shots"
    },
    {
      "name": "max_shots_constraint",
      "expr_lhs": "batches['shots']",
      "sense": "<=",
      "expr_rhs": "10",
      "description": "Maximum number of batches of shots"
    }
  ],
  "graph": null
}