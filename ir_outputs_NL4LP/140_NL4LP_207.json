{
  "meta": {
    "problem_id": "NL4LP_207",
    "source": null,
    "description": "Optimize the number of regular and deluxe tacos to maximize profit given demand and supply constraints.",
    "sense": "max",
    "version": 1
  },
  "sets": [
    {
      "name": "TACOS",
      "elements": [
        "regular",
        "deluxe"
      ],
      "description": "Types of tacos sold at the stand"
    }
  ],
  "params": [
    {
      "name": "profit",
      "indices": [
        "TACOS"
      ],
      "values": {
        "regular": 2.5,
        "deluxe": 3.55
      },
      "description": "Profit per taco type"
    },
    {
      "name": "demand",
      "indices": [
        "TACOS"
      ],
      "values": {
        "regular": 50,
        "deluxe": 40
      },
      "description": "Maximum demand for each type of taco"
    },
    {
      "name": "supply_limit",
      "indices": [],
      "values": 70,
      "description": "Total supply limit for tacos"
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "TACOS"
      ],
      "vartype": "integer",
      "lb": 0.0,
      "ub": null,
      "description": "Number of tacos to make of each type"
    }
  ],
  "objective": {
    "name": "max_profit",
    "sense": "max",
    "expr": "x['regular'] * profit['regular'] + x['deluxe'] * profit['deluxe']",
    "description": "Maximize total profit from selling tacos"
  },
  "constraints": [
    {
      "name": "demand_regular",
      "expr_lhs": "x['regular']",
      "sense": "<=",
      "expr_rhs": "demand['regular']",
      "description": "Demand constraint for regular tacos"
    },
    {
      "name": "demand_deluxe",
      "expr_lhs": "x['deluxe']",
      "sense": "<=",
      "expr_rhs": "demand['deluxe']",
      "description": "Demand constraint for deluxe tacos"
    },
    {
      "name": "supply_limit",
      "expr_lhs": "x['regular'] + x['deluxe']",
      "sense": "<=",
      "expr_rhs": "supply_limit",
      "description": "Total supply constraint for tacos"
    }
  ],
  "graph": null
