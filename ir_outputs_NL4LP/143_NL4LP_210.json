{
  "meta": {
    "problem_id": "NL4LP_210",
    "source": null,
    "description": "Optimize the production of software packages to maximize profits under budget and sales constraints.",
    "sense": "max",
    "version": 1
  },
  "sets": [
    {
      "name": "PACKAGES",
      "elements": [
        "personal",
        "commercial"
      ],
      "description": "Types of software packages"
    }
  ],
  "params": [
    {
      "name": "cost",
      "indices": [
        "PACKAGES"
      ],
      "values": {
        "personal": 550,
        "commercial": 2000
      },
      "description": "Cost to generate each type of software package"
    },
    {
      "name": "profit",
      "indices": [
        "PACKAGES"
      ],
      "values": {
        "personal": 450,
        "commercial": 1200
      },
      "description": "Profit per software package"
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "PACKAGES"
      ],
      "vartype": "integer",
      "lb": 0.0,
      "ub": null,
      "description": "Number of each software package to produce"
    }
  ],
  "objective": {
    "name": "max_total_profit",
    "sense": "max",
    "expr": "450 * x['personal'] + 1200 * x['commercial']",
    "description": "Maximize total profit from software packages"
  },
  "constraints": [
    {
      "name": "budget_constraint",
      "expr_lhs": "550 * x['personal'] + 2000 * x['commercial']",
      "sense": "<=",
      "expr_rhs": "400000",
      "description": "Total cost must not exceed $400,000"
    },
    {
      "name": "sales_limit_constraint",
      "expr_lhs": "x['personal'] + x['commercial']",
      "sense": "<=",
      "expr_rhs": "300",
      "description": "Total sales must not exceed 300 licenses"
    }
  ],
  "graph": null
}