{
  "meta": {
    "problem_id": "NL4LP_21",
    "source": null,
    "description": "Optimize the production of regular and premium car models to maximize profit.",
    "sense": "max",
    "version": 1
  },
  "sets": [
    {
      "name": "MODELS",
      "elements": [
        "regular",
        "premium"
      ],
      "description": "Types of car models produced."
    }
  ],
  "params": [
    {
      "name": "profit",
      "indices": [
        "MODELS"
      ],
      "values": {
        "regular": 5000,
        "premium": 8500
      },
      "description": "Profit per car model."
    },
    {
      "name": "demand_limit",
      "indices": [
        "MODELS"
      ],
      "values": {
        "regular": 8,
        "premium": 6
      },
      "description": "Maximum daily demand for each car model."
    },
    {
      "name": "total_production_limit",
      "indices": [],
      "values": 12,
      "description": "Maximum total number of cars that can be produced per day."
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "MODELS"
      ],
      "vartype": "integer",
      "lb": 0.0,
      "ub": null,
      "description": "Number of cars produced per model per day."
    }
  ],
  "objective": {
    "name": "max_profit",
    "sense": "max",
    "expr": "5000 * x['regular'] + 8500 * x['premium']",
    "description": "Maximize the total profit from car production."
  },
  "constraints": [
    {
      "name": "demand_regular",
      "expr_lhs": "x['regular']",
      "sense": "<=",
      "expr_rhs": "8",
      "description": "Demand limit for regular models."
    },
    {
      "name": "demand_premium",
      "expr_lhs": "x['premium']",
      "sense": "<=",
      "expr_rhs": "6",
      "description": "Demand limit for premium models."
    },
    {
      "name": "total_production",
      "expr_lhs": "x['regular'] + x['premium']",
      "sense": "<=",
      "expr_rhs": "12",
      "description": "Total production limit for all car models."
    }
  ],
  "graph": null
}