{
  "meta": {
    "problem_id": "NL4LP_96",
    "source": null,
    "description": "Optimize the number of fish and chicken meals to minimize fat intake while meeting protein and iron requirements.",
    "sense": "min",
    "version": 1
  },
  "sets": [
    {
      "name": "MEALS",
      "elements": [
        "fish",
        "chicken"
      ],
      "description": "Types of meals available"
    }
  ],
  "params": [
    {
      "name": "protein",
      "indices": [
        "MEALS"
      ],
      "values": {
        "fish": 10,
        "chicken": 15
      },
      "description": "Protein content per meal type"
    },
    {
      "name": "iron",
      "indices": [
        "MEALS"
      ],
      "values": {
        "fish": 12,
        "chicken": 8
      },
      "description": "Iron content per meal type"
    },
    {
      "name": "fat",
      "indices": [
        "MEALS"
      ],
      "values": {
        "fish": 7,
        "chicken": 10
      },
      "description": "Fat content per meal type"
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "MEALS"
      ],
      "vartype": "integer",
      "lb": 0.0,
      "ub": null,
      "description": "Number of meals of each type"
    }
  ],
  "objective": {
    "name": "min_total_fat",
    "sense": "min",
    "expr": "fat['fish'] * x['fish'] + fat['chicken'] * x['chicken']",
    "description": "Minimize total fat intake from meals"
  },
  "constraints": [
    {
      "name": "protein_requirement",
      "expr_lhs": "protein['fish'] * x['fish'] + protein['chicken'] * x['chicken']",
      "sense": ">=",
      "expr_rhs": "130",
      "description": "Ensure at least 130 units of protein intake"
    },
    {
      "name": "iron_requirement",
      "expr_lhs": "iron['fish'] * x['fish'] + iron['chicken'] * x['chicken']",
      "sense": ">=",
      "expr_rhs": "120",
      "description": "Ensure at least 120 units of iron intake"
    },
    {
      "name": "chicken_preference",
      "expr_lhs": "x['chicken']",
      "sense": ">=",
      "expr_rhs": "2 * x['fish']",
      "description": "Consume at least twice as many chicken meals as fish meals"
    }
  ],
  "graph": null
}