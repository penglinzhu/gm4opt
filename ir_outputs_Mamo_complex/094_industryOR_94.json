  "objective": {
    "name": "min_total_cost",
    "sense": "min",
    "expr": "sum(opening_cost[c] * open_center[c] for c in CENTERS) + sum(transport_cost[c][o] * shipment[c][o] for c in CENTERS for o in OUTLETS)",
    "description": "Minimize the total cost including opening and transportation costs"
  },
  "constraints": [
    {
      "name": "demand_satisfaction_1",
      "expr_lhs": "sum(shipment[c]['1'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['1']",
      "description": "Satisfy demand at outlet 1"
    },
    {
      "name": "demand_satisfaction_2",
      "expr_lhs": "sum(shipment[c]['2'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['2']",
      "description": "Satisfy demand at outlet 2"
    },
    {
      "name": "demand_satisfaction_3",
      "expr_lhs": "sum(shipment[c]['3'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['3']",
      "description": "Satisfy demand at outlet 3"
    },
    {
      "name": "demand_satisfaction_4",
      "expr_lhs": "sum(shipment[c]['4'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['4']",
      "description": "Satisfy demand at outlet 4"
    },
    {
      "name": "capacity_limit_1",
      "expr_lhs": "sum(shipment['1'][o] for o in OUTLETS)",
      "sense": "<=",
      "expr_rhs": "capacity['1'] * open_center['1']",
      "description": "Capacity limit for center 1"
    },
    {
      "name": "capacity_limit_2",
