      "name": "task_assignment",
      "expr_lhs": "sum(x[t][j] for j in TASKS if j != t)",
      "sense": "==",
      "expr_rhs": "y[t]",
      "description": "Each task must be assigned to exactly one driver."
    },
    {
      "name": "driver_continuity",
      "expr_lhs": "sum(x[i][t] for i in TASKS if i != t)",
      "sense": "==",
      "expr_rhs": "y[t]",
      "description": "Ensure continuity of driver assignments."
    }
