    }
  ],
  "objective": {
    "name": "min_total_cost",
    "sense": "min",
    "expr": "sum(opening_cost[c] * open_center[c] for c in CENTERS) + sum(transport_cost[c][s] * shipment[c][s] for c in CENTERS for s in STORES)",
    "description": "Minimize the total cost including opening and transportation costs"
  },
  "constraints": [
    {
      "name": "demand_satisfaction_store_1",
      "expr_lhs": "sum(shipment[c]['1'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['1']",
      "description": "Satisfy demand for store 1"
    },
    {
      "name": "demand_satisfaction_store_2",
      "expr_lhs": "sum(shipment[c]['2'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['2']",
      "description": "Satisfy demand for store 2"
    },
    {
      "name": "demand_satisfaction_store_3",
      "expr_lhs": "sum(shipment[c]['3'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['3']",
      "description": "Satisfy demand for store 3"
    },
    {
      "name": "demand_satisfaction_store_4",
      "expr_lhs": "sum(shipment[c]['4'] for c in CENTERS)",
      "sense": ">=",
      "expr_rhs": "demand['4']",
      "description": "Satisfy demand for store 4"
    },
    {
      "name": "capacity_limit_center_1",
      "expr_lhs": "sum(shipment['1'][s] for s in STORES)",
      "sense": "<=",
      "expr_rhs": "capacity['1'] * open_center['1']",
      "description": "Capacity limit for center 1"
    },
    {
      "name": "capacity_limit_center_2",
      "expr_lhs": "sum(shipment['2'][s] for s in STORES)",
      "sense": "<=",
      "expr_rhs": "capacity['2'] * open_center['2']",
      "description": "Capacity limit for center 2"
    },
    {
      "name": "capacity_limit_center_3",
      "expr_lhs": "sum(shipment['3'][s] for s in STORES)",
      "sense": "<=",
      "expr_rhs": "capacity['3'] * open_center['3']",
