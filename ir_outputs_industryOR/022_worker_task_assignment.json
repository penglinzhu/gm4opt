{
  "meta": {
    "problem_id": "worker_task_assignment",
    "source": null,
    "description": "Determine 4 out of 5 workers to complete one of the four tasks, minimizing total working hours.",
    "sense": "min",
    "version": 1
  },
  "sets": [
    {
      "name": "WORKERS",
      "elements": [
        "I",
        "II",
        "III",
        "IV",
        "V"
      ],
      "description": "Available workers"
    },
    {
      "name": "TASKS",
      "elements": [
        "A",
        "B",
        "C",
        "D"
      ],
      "description": "Tasks to be completed"
    }
  ],
  "params": [
    {
      "name": "time",
      "indices": [
        "WORKERS",
        "TASKS"
      ],
      "values": {
        "I": {
          "A": 9,
          "B": 4,
          "C": 3,
          "D": 7
        },
        "II": {
          "A": 4,
          "B": 6,
          "C": 5,
          "D": 6
        },
        "III": {
          "A": 5,
          "B": 4,
          "C": 7,
          "D": 5
        },
        "IV": {
          "A": 7,
          "B": 5,
          "C": 2,
          "D": 3
        },
        "V": {
          "A": 10,
          "B": 6,
          "C": 7,
          "D": 4
        }
      },
      "description": "Time required for each worker to complete each task"
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "WORKERS",
        "TASKS"
      ],
      "vartype": "binary",
      "lb": 0.0,
      "ub": 1.0,
      "description": "1 if worker is assigned to task, 0 otherwise"
    }
  ],
  "objective": {
    "name": "min_total_time",
    "sense": "min",
    "expr": "sum(time[w][t] * x[w][t] for w in WORKERS for t in TASKS)",
    "description": "Minimize total working hours"
  },
  "constraints": [
    {
      "name": "task_A",
      "expr_lhs": "x['I']['A'] + x['II']['A'] + x['III']['A'] + x['IV']['A'] + x['V']['A']",
      "sense": "==",
      "expr_rhs": "1",
      "description": "Task A assigned to exactly one worker"
    },
    {
      "name": "task_B",
      "expr_lhs": "x['I']['B'] + x['II']['B'] + x['III']['B'] + x['IV']['B'] + x['V']['B']",
      "sense": "==",
      "expr_rhs": "1",
      "description": "Task B assigned to exactly one worker"
    },
    {
      "name": "task_C",
      "expr_lhs": "x['I']['C'] + x['II']['C'] + x['III']['C'] + x['IV']['C'] + x['V']['C']",
      "sense": "==",
      "expr_rhs": "1",
      "description": "Task C assigned to exactly one worker"
    },
    {
      "name": "task_D",
      "expr_lhs": "x['I']['D'] + x['II']['D'] + x['III']['D'] + x['IV']['D'] + x['V']['D']",
      "sense": "==",
      "expr_rhs": "1",
      "description": "Task D assigned to exactly one worker"
    },
    {
      "name": "worker_I",
      "expr_lhs": "x['I']['A'] + x['I']['B'] + x['I']['C'] + x['I']['D']",
      "sense": "<=",
      "expr_rhs": "1",
      "description": "Worker I assigned to at most one task"
    },
    {
      "name": "worker_II",
      "expr_lhs": "x['II']['A'] + x['II']['B'] + x['II']['C'] + x['II']['D']",
      "sense": "<=",
      "expr_rhs": "1",
      "description": "Worker II assigned to at most one task"
    },
    {
      "name": "worker_III",
      "expr_lhs": "x['III']['A'] + x['III']['B'] + x['III']['C'] + x['III']['D']",
      "sense": "<=",
      "expr_rhs": "1",
      "description": "Worker III assigned to at most one task"
    },
    {
      "name": "worker_IV",
      "expr_lhs": "x['IV']['A'] + x['IV']['B'] + x['IV']['C'] + x['IV']['D']",
      "sense": "<=",
      "expr_rhs": "1",
      "description": "Worker IV assigned to at most one task"
    },
    {
      "name": "worker_V",
      "expr_lhs": "x['V']['A'] + x['V']['B'] + x['V']['C'] + x['V']['D']",
      "sense": "<=",
      "expr_rhs": "1",
      "description": "Worker V assigned to at most one task"
    },
    {
      "name": "select_4_workers",
      "expr_lhs": "x['I']['A'] + x['I']['B'] + x['I']['C'] + x['I']['D'] + x['II']['A'] + x['II']['B'] + x['II']['C'] + x['II']['D'] + x['III']['A'] + x['III']['B'] + x['III']['C'] + x['III']['D'] + x['IV']['A'] + x['IV']['B'] + x['IV']['C'] + x['IV']['D'] + x['V']['A'] + x['V']['B'] + x['V']['C'] + x['V']['D']",
      "sense": "==",
      "expr_rhs": "4",
      "description": "Exactly 4 workers are selected"
    }
  ],
