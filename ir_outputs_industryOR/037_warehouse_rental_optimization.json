{
  "meta": {
    "problem_id": "warehouse_rental_optimization",
    "source": null,
    "description": "Minimize the rental fees for warehouse storage over a 4-month period with varying contract lengths and discounts.",
    "sense": "min",
    "version": 1
  },
  "sets": [
    {
      "name": "MONTHS",
      "elements": [
        "1",
        "2",
        "3",
        "4"
      ],
      "description": "Months in the planning horizon"
    },
    {
      "name": "CONTRACT_PERIODS",
      "elements": [
        "1",
        "2",
        "3",
        "4"
      ],
      "description": "Possible contract rental periods in months"
    }
  ],
  "params": [
    {
      "name": "required_area",
      "indices": [
        "MONTHS"
      ],
      "values": {
        "1": 1500,
        "2": 1000,
        "3": 2000,
        "4": 1200
      },
      "description": "Required warehouse area for each month in m^2"
    },
    {
      "name": "rental_fee",
      "indices": [
        "CONTRACT_PERIODS"
      ],
      "values": {
        "1": 28,
        "2": 45,
        "3": 60,
        "4": 73
      },
      "description": "Rental fee per m^2 for each contract period"
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "MONTHS",
        "CONTRACT_PERIODS"
      ],
      "vartype": "continuous",
      "lb": 0.0,
      "ub": null,
      "description": "Area rented in m^2 for each month and contract period"
    }
  ],
  "objective": {
    "name": "min_total_rental_cost",
    "sense": "min",
    "expr": "sum(rental_fee[p] * x[m][p] for m in MONTHS for p in CONTRACT_PERIODS)",
    "description": "Minimize the total rental cost over all months and contract periods"
  },
  "constraints": [
    {
      "name": "demand_coverage_1",
      "expr_lhs": "x['1']['1'] + x['1']['2'] + x['1']['3'] + x['1']['4']",
      "sense": ">=",
      "expr_rhs": "required_area['1']",
      "description": "Meet required area for month 1"
    },
    {
      "name": "demand_coverage_2",
      "expr_lhs": "x['2']['1'] + x['2']['2'] + x['2']['3'] + x['2']['4']",
      "sense": ">=",
      "expr_rhs": "required_area['2']",
      "description": "Meet required area for month 2"
    },
    {
      "name": "demand_coverage_3",
      "expr_lhs": "x['3']['1'] + x['3']['2'] + x['3']['3'] + x['3']['4']",
      "sense": ">=",
      "expr_rhs": "required_area['3']",
      "description": "Meet required area for month 3"
    },
    {
      "name": "demand_coverage_4",
      "expr_lhs": "x['4']['1'] + x['4']['2'] + x['4']['3'] + x['4']['4']",
      "sense": ">=",
      "expr_rhs": "required_area['4']",
      "description": "Meet required area for month 4"
    }
  ],
  "graph": null
}