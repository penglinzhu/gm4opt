{
  "meta": {
    "problem_id": "industryOR_41",
    "source": null,
    "description": "Determine the minimum number of waitstaff required to meet the minimum staffing requirements for each time period in a 24-hour restaurant operation.",
    "sense": "min",
    "version": 1
  },
  "sets": [
    {
      "name": "TIME_PERIODS",
      "elements": [
        "2am-6am",
        "6am-10am",
        "10am-2pm",
        "2pm-6pm",
        "6pm-10pm",
        "10pm-2am"
      ],
      "description": "Time periods during which the restaurant operates."
    }
  ],
  "params": [
    {
      "name": "min_waitstaff",
      "indices": [
        "TIME_PERIODS"
      ],
      "values": {
        "2am-6am": 4,
        "6am-10am": 8,
        "10am-2pm": 10,
        "2pm-6pm": 4,
        "6pm-10pm": 8,
        "10pm-2am": 4
      },
      "description": "Minimum number of waitstaff required for each time period."
    }
  ],
  "vars": [
    {
      "name": "x",
      "indices": [
        "TIME_PERIODS"
      ],
      "vartype": "integer",
      "lb": 0.0,
      "ub": null,
      "description": "Number of waitstaff starting their shift at the beginning of each time period."
    }
  ],
  "objective": {
    "name": "min_total_waitstaff",
    "sense": "min",
    "expr": "sum(x[t] for t in ['2am-6am', '6am-10am', '10am-2pm', '2pm-6pm', '6pm-10pm', '10pm-2am'])",
    "description": "Minimize the total number of waitstaff required."
  },
  "constraints": [
    {
      "name": "min_staff_2am_6am",
      "expr_lhs": "x['10pm-2am'] + x['2am-6am']",
      "sense": ">=",
      "expr_rhs": "min_waitstaff['2am-6am']",
      "description": "Ensure minimum waitstaff requirement for 2am-6am."
    },
    {
      "name": "min_staff_6am_10am",
      "expr_lhs": "x['2am-6am'] + x['6am-10am']",
      "sense": ">=",
      "expr_rhs": "min_waitstaff['6am-10am']",
      "description": "Ensure minimum waitstaff requirement for 6am-10am."
    },
    {
      "name": "min_staff_10am_2pm",
      "expr_lhs": "x['6am-10am'] + x['10am-2pm']",
      "sense": ">=",
      "expr_rhs": "min_waitstaff['10am-2pm']",
      "description": "Ensure minimum waitstaff requirement for 10am-2pm."
    },
    {
      "name": "min_staff_2pm_6pm",
      "expr_lhs": "x['10am-2pm'] + x['2pm-6pm']",
      "sense": ">=",
      "expr_rhs": "min_waitstaff['2pm-6pm']",
      "description": "Ensure minimum waitstaff requirement for 2pm-6pm."
    },
    {
      "name": "min_staff_6pm_10pm",
      "expr_lhs": "x['2pm-6pm'] + x['6pm-10pm']",
      "sense": ">=",
      "expr_rhs": "min_waitstaff['6pm-10pm']",
      "description": "Ensure minimum waitstaff requirement for 6pm-10pm."
    },
    {
      "name": "min_staff_10pm_2am",
      "expr_lhs": "x['6pm-10pm'] + x['10pm-2am']",
      "sense": ">=",
      "expr_rhs": "min_waitstaff['10pm-2am']",
      "description": "Ensure minimum waitstaff requirement for 10pm-2am."
    }
  ],
  "graph": null
}