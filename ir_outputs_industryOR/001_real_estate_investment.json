    "description": "Maximize the annual income from purchasing properties within a budget constraint.",
    "sense": "max",
    "version": 1
  },
  "sets": [
    {
      "name": "PROPERTIES",
      "elements": [
        "1",
        "2",
        "3",
        "4"
      ],
      "description": "Available properties for investment"
    }
  ],
  "params": [
    {
      "name": "annual_income",
      "indices": [
        "PROPERTIES"
      ],
      "values": {
        "1": 12500,
        "2": 35000,
        "3": 23000,
        "4": 100000
      },
      "description": "Annual income from each property"
    },
    {
      "name": "cost",
      "indices": [
        "PROPERTIES"
      ],
      "values": {
        "1": 1500000,
        "2": 2100000,
        "3": 2300000,
        "4": 4200000
      },
      "description": "Cost of each property"
    },
    {
      "name": "budget",
      "indices": [],
      "values": 7000000,
      "description": "Total budget available for investment"
    }
  ],
  "vars": [
    {
      "name": "buy",
      "indices": [
        "PROPERTIES"
      ],
      "vartype": "binary",
      "lb": 0.0,
      "ub": 1.0,
      "description": "Decision to buy each property"
    }
  ],
  "objective": {
    "name": "max_annual_income",
    "sense": "max",
    "expr": "sum(annual_income[p] * buy[p] for p in PROPERTIES)",
    "description": "Maximize the total annual income from purchased properties"
  },
  "constraints": [
    {
      "name": "budget_constraint",
      "expr_lhs": "sum(cost[p] * buy[p] for p in PROPERTIES)",
      "sense": "<=",
      "expr_rhs": "budget",
      "description": "Total cost of purchased properties must not exceed the budget"
    },
    {
      "name": "mutual_exclusivity_3_4",
      "expr_lhs": "buy['3'] + buy['4']",
      "sense": "<=",
      "expr_rhs": "1",
      "description": "Cannot purchase both Property 3 and Property 4"
    }
  ],
