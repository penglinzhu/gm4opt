      "vartype": "continuous",
      "lb": 0.0,
      "ub": null,
      "description": "Total overtime hours worked each month"
    }
  ],
  "objective": {
    "name": "max_total_net_profit",
    "sense": "max",
    "expr": "sum((unit_price * (production[m] + outsourced[m]) - raw_material_cost * production[m] - normal_wage * normal_hours_per_day * working_days_per_month * workers[m] - overtime_wage * overtime_hours[m] - outsourcing_cost * outsourced[m] - inventory_cost * inventory[m] - stockout_cost * stockout[m] - hiring_cost * hired[m] - dismissal_cost * dismissed[m]) for m in MONTHS)",
    "description": "Maximize total net profit over 6 months"
  },
  "constraints": [
    {
      "name": "initial_inventory_balance",
      "expr_lhs": "inventory['1']",
      "sense": "==",
      "expr_rhs": "initial_inventory + production['1'] + outsourced['1'] - demand_forecast['1'] + stockout['1']",
      "description": "Inventory balance for month 1"
    },
    {
      "name": "inventory_balance_2",
      "expr_lhs": "inventory['2']",
      "sense": "==",
      "expr_rhs": "inventory['1'] + production['2'] + outsourced['2'] - demand_forecast['2'] + stockout['2']",
      "description": "Inventory balance for month 2"
    },
    {
      "name": "inventory_balance_3",
      "expr_lhs": "inventory['3']",
      "sense": "==",
      "expr_rhs": "inventory['2'] + production['3'] + outsourced['3'] - demand_forecast['3'] + stockout['3']",
      "description": "Inventory balance for month 3"
    },
    {
      "name": "inventory_balance_4",
      "expr_lhs": "inventory['4']",
      "sense": "==",
      "expr_rhs": "inventory['3'] + production['4'] + outsourced['4'] - demand_forecast['4'] + stockout['4']",
      "description": "Inventory balance for month 4"
    },
    {
      "name": "inventory_balance_5",
      "expr_lhs": "inventory['5']",
      "sense": "==",
      "expr_rhs": "inventory['4'] + production['5'] + outsourced['5'] - demand_forecast['5'] + stockout['5']",
      "description": "Inventory balance for month 5"
    },
    {
      "name": "inventory_balance_6",
      "expr_lhs": "inventory['6']",
      "sense": "==",
      "expr_rhs": "inventory['5'] + production['6'] + outsourced['6'] - demand_forecast['6'] + stockout['6']",
      "description": "Inventory balance for month 6"
    },
    {
      "name": "end_inventory_requirement",
      "expr_lhs": "inventory['6']",
      "sense": ">=",
      "expr_rhs": "10000",
      "description": "Ensure at least 10,000 units of inventory by end of June"
    },
    {
      "name": "worker_balance_1",
      "expr_lhs": "workers['1']",
      "sense": "==",
      "expr_rhs": "initial_workers + hired['1'] - dismissed['1']",
      "description": "Worker balance for month 1"
    },
    {
      "name": "worker_balance_2",
      "expr_lhs": "workers['2']",
      "sense": "==",
      "expr_rhs": "workers['1'] + hired['2'] - dismissed['2']",
      "description": "Worker balance for month 2"
    },
    {
      "name": "worker_balance_3",
      "expr_lhs": "workers['3']",
      "sense": "==",
      "expr_rhs": "workers['2'] + hired['3'] - dismissed['3']",
      "description": "Worker balance for month 3"
    },
    {
      "name": "worker_balance_4",
      "expr_lhs": "workers['4']",
      "sense": "==",
      "expr_rhs": "workers['3'] + hired['4'] - dismissed['4']",
      "description": "Worker balance for month 4"
    },
    {
      "name": "worker_balance_5",
      "expr_lhs": "workers['5']",
      "sense": "==",
      "expr_rhs": "workers['4'] + hired['5'] - dismissed['5']",
      "description": "Worker balance for month 5"
    },
    {
      "name": "worker_balance_6",
      "expr_lhs": "workers['6']",
      "sense": "==",
      "expr_rhs": "workers['5'] + hired['6'] - dismissed['6']",
      "description": "Worker balance for month 6"
    },
    {
      "name": "production_capacity_1",
      "expr_lhs": "production['1']",
      "sense": "<=",
      "expr_rhs": "workers['1'] * (normal_hours_per_day * working_days_per_month + overtime_hours['1']) / labor_hours_per_unit",
      "description": "Production capacity constraint for month 1"
    },
    {
      "name": "production_capacity_2",
      "expr_lhs": "production['2']",
      "sense": "<=",
      "expr_rhs": "workers['2'] * (normal_hours_per_day * working_days_per_month + overtime_hours['2']) / labor_hours_per_unit",
      "description": "Production capacity constraint for month 2"
    },
    {
      "name": "production_capacity_3",
      "expr_lhs": "production['3']",
      "sense": "<=",
      "expr_rhs": "workers['3'] * (normal_hours_per_day * working_days_per_month + overtime_hours['3']) / labor_hours_per_unit",
      "description": "Production capacity constraint for month 3"
    },
    {
      "name": "production_capacity_4",
      "expr_lhs": "production['4']",
      "sense": "<=",
      "expr_rhs": "workers['4'] * (normal_hours_per_day * working_days_per_month + overtime_hours['4']) / labor_hours_per_unit",
      "description": "Production capacity constraint for month 4"
    },
    {
      "name": "production_capacity_5",
      "expr_lhs": "production['5']",
      "sense": "<=",
      "expr_rhs": "workers['5'] * (normal_hours_per_day * working_days_per_month + overtime_hours['5']) / labor_hours_per_unit",
      "description": "Production capacity constraint for month 5"
    },
    {
      "name": "production_capacity_6",
      "expr_lhs": "production['6']",
      "sense": "<=",
      "expr_rhs": "workers['6'] * (normal_hours_per_day * working_days_per_month + overtime_hours['6']) / labor_hours_per_unit",
      "description": "Production capacity constraint for month 6"
    },
    {
      "name": "overtime_limit_1",
      "expr_lhs": "overtime_hours['1']",
      "sense": "<=",
      "expr_rhs": "workers['1'] * max_overtime_hours",
      "description": "Overtime limit for month 1"
    },
    {
      "name": "overtime_limit_2",
      "expr_lhs": "overtime_hours['2']",
      "sense": "<=",
      "expr_rhs": "workers['2'] * max_overtime_hours",
      "description": "Overtime limit for month 2"
    },
    {
      "name": "overtime_limit_3",
      "expr_lhs": "overtime_hours['3']",
      "sense": "<=",
      "expr_rhs": "workers['3'] * max_overtime_hours",
      "description": "Overtime limit for month 3"
    },
    {
      "name": "overtime_limit_4",
      "expr_lhs": "overtime_hours['4']",
      "sense": "<=",
      "expr_rhs": "workers['4'] * max_overtime_hours",
      "description": "Overtime limit for month 4"
    },
    {
      "name": "overtime_limit_5",
      "expr_lhs": "overtime_hours['5']",
      "sense": "<=",
      "expr_rhs": "workers['5'] * max_overtime_hours",
